{
  "mcpServers": {
    "elastic-otel": {
      "command": "/opt/o11y-mcp-server/venv/bin/python",
      "args": [
        "/opt/o11y-mcp-server/elastic-otel-mcp-server.py"
      ],
      "env": {
        "ELASTIC_ENDPOINT": "${ELASTIC_ENDPOINT}",
        "ELASTIC_API_KEY": "${ELASTIC_API_KEY}"
      }
    }
  }
}
